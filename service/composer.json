{
    "name": "thomasbley/ide_in_docker",
    "description": "PHP Example",
    "license": "MIT",
    "config": {
        "secure-http": true,
        "platform": {
            "php": "8.0"
        },
        "preferred-install" : "dist",
        "vendor-dir": "src/vendor"
    },
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*"
    },
    "autoload": {
        "psr-4": { "Service\\": "src/" }
    },
    "prefer-stable" : true
}
